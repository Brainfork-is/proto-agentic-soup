    "fastify": "^4.27.2",
    "ioredis": "^5.4.1",
    "@prisma/client": "^5.17.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "tsx": "^4.16.2",
    "typescript": "^5.5.0",
    "prisma": "^5.17.0"
  }
}
